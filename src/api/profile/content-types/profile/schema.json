{
  "kind": "collectionType",
  "collectionName": "profiles",
  "info": {
    "singularName": "profile",
    "pluralName": "profiles",
    "displayName": "Profile",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "profilePhoto": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": ["images"]
    },
    "slug": {
      "type": "string",
      "required": true
    },
    "domain": {
      "type": "enumeration",
      "enum": [
        "@ytlcement.com",
        "@ytlcement.com.my",
        "@ytlcement.com.sg",
        "@fico-ytl.com",
        "@ytlcement.com.mm"
      ],
      "required": true
    },
    "jobTitle": {
      "type": "string",
      "required": true
    },
    "company": {
      "type": "string",
      "required": true,
      "default": "YTL Cement Berhad"
    },
    "countryCodeMobile": {
      "type": "enumeration",
      "enum": ["Malaysia (60)", "Singapore (65)", "Vietnam (84)"],
      "required": false
    },
    "mobileNumber": {
      "type": "string"
    },
    "countryCodeOffice": {
      "type": "enumeration",
      "enum": ["Malaysia (60)", "Singapore (65)", "Vietnam (84)"]
    },
    "officeNumber": {
      "type": "string"
    },
    "extensionNumber": {
      "type": "biginteger"
    },
    "location": {
      "type": "text"
    },
    "about": {
      "type": "text"
    }
  }
}
