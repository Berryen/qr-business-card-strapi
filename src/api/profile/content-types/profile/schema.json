{
  "kind": "collectionType",
  "collectionName": "profiles",
  "info": {
    "singularName": "profile",
    "pluralName": "profiles",
    "displayName": "Profile",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true,
      "pluginOptions": {}
    },
    "profilePhoto": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ],
      "pluginOptions": {}
    },
    "slug": {
      "type": "string",
      "required": true
    },
    "emailId": {
      "type": "string",
      "required": true,
      "pluginOptions": {}
    },
    "domain": {
      "type": "enumeration",
      "enum": [
        "@ytlcement.com",
        "@ytlcement.com.my",
        "@ytlcement.com.sg",
        "@fico-ytl.com",
        "@ytlcement.com.mm"
      ],
      "required": true,
      "pluginOptions": {}
    },
    "jobTitle": {
      "type": "string",
      "required": true,
      "pluginOptions": {}
    },
    "company": {
      "type": "string",
      "required": true,
      "default": "YTL Cement Berhad",
      "pluginOptions": {}
    },
    "countryCodeMobile": {
      "type": "enumeration",
      "enum": [
        "Malaysia (60)",
        "Singapore (65)",
        "Vietnam (84)"
      ],
      "required": false,
      "pluginOptions": {}
    },
    "mobileNumber": {
      "type": "string",
      "pluginOptions": {}
    },
    "countryCodeOffice": {
      "type": "enumeration",
      "enum": [
        "Malaysia (60)",
        "Singapore (65)",
        "Vietnam (84)"
      ],
      "pluginOptions": {}
    },
    "officeNumber": {
      "type": "string",
      "pluginOptions": {}
    },
    "extensionNumber": {
      "type": "biginteger",
      "pluginOptions": {}
    },
    "linkedIn": {
      "type": "string",
      "pluginOptions": {}
    },
    "location": {
      "type": "text",
      "pluginOptions": {},
      "required": true
    },
    "about": {
      "type": "text",
      "maxLength": 150,
      "pluginOptions": {}
    },
    "showWhatsapp": {
      "pluginOptions": {},
      "type": "boolean",
      "default": true
    }
  }
}
