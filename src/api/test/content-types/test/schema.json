{
  "kind": "collectionType",
  "collectionName": "tests",
  "info": {
    "singularName": "test",
    "pluralName": "tests",
    "displayName": "Profile Card",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "profilePhoto": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "slug": {
      "type": "string",
      "required": true
    },
    "jobTitle": {
      "type": "string",
      "required": true
    },
    "company": {
      "type": "string",
      "required": true
    },
    "countryCodeMobile": {
      "type": "enumeration",
      "enum": [
        "Malaysia (60)",
        "Singapore (65)",
        "Vietnam (84)"
      ]
    },
    "mobileNumber": {
      "type": "string"
    },
    "countryCodeOffice": {
      "type": "enumeration",
      "enum": [
        "Malaysia (60)",
        "Singapore (65)",
        "Vietnam (84)"
      ]
    },
    "officeNumber": {
      "type": "string"
    },
    "extensionNumber": {
      "type": "biginteger"
    },
    "linkedIn": {
      "type": "string"
    },
    "location": {
      "type": "text"
    },
    "about": {
      "type": "text"
    },
    "showWhatsapp": {
      "type": "boolean",
      "default": true
    },
    "email": {
      "type": "email",
      "required": true
    }
  }
}
